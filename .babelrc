{
    "presets": [
        ["@babel/preset-env", { "modules": false }],
        "@babel/preset-react",
        [
            "@babel/preset-typescript",
            { "isTSX": true, "allExtensions": true }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ],
        "@babel/plugin-proposal-object-rest-spread",
    ]
}
