{
    "name": "@titan-tooling/ui",
    "version": "0.6.2",
    "description": "Titan UI CSS",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/code-mattclaffey/titan-ui"
    },
    "scripts": {
        "start": "npm run build && npm run storybook",
        "build": "npm run build:css && npm run build:js",
        "build:css": "postcss ./src/css/main.css -o ./dist/css/main.css -m",
        "serve": "http-server storybook-static -p 8081 -s",
        "test": "npm run lint:js && npm run jest",
        "jest": "jest",
        "storybook": "npm run build && start-storybook -s ./dist -p 6006",
        "deploy": "npm run test && npm run build && npm run build-storybook",
        "build-storybook": "build-storybook -s ./dist",
        "copy": "",
        "snapshot": "build-storybook -s ./dist && percy-storybook --widths=320,1300",
        "build:js": "babel src --extensions '.ts','.tsx'  --ignore 'src/components/**/*.test.ts','src/components/**/*.test.tsx','src/components/**/*.stories.tsx' --out-dir dist",
        "lint:js": "eslint src/components/**/*.tsx"
    },
    "license": "UNLICENSED",
    "dependencies": {
        "normalize.css": "^8.0.1"
    },
    "peerDependencies": {
        "classnames": "^2.2.6",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-syntax-class-properties": "^7.7.4",
        "@babel/plugin-syntax-dynamic-import": "^7.7.4",
        "@babel/plugin-transform-modules-commonjs": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "@percy/storybook": "^3.2.0",
        "@storybook/addon-a11y": "^5.2.6",
        "@storybook/addon-actions": "^5.2.6",
        "@storybook/addon-console": "^1.2.1",
        "@storybook/addon-docs": "^5.2.6",
        "@storybook/addon-links": "^5.2.6",
        "@storybook/addon-viewport": "^5.2.6",
        "@storybook/addons": "^5.2.6",
        "@storybook/react": "^5.2.6",
        "@storybook/theming": "^5.2.6",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@types/classnames": "^2.2.9",
        "@types/jest": "^24.0.23",
        "@types/react": "^16.9.16",
        "@types/react-dom": "^16.9.4",
        "@typescript-eslint/eslint-plugin": "^2.11.0",
        "@typescript-eslint/parser": "^2.11.0",
        "autoprefixer": "^9.7.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.2.0",
        "cssnano": "^4.1.10",
        "eslint": "^6.7.1",
        "eslint-config-airbnb-typescript": "^6.3.1",
        "eslint-config-airbnb-typescript-prettier": "^1.4.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "http-server": "^0.11.1",
        "jest": "^24.9.0",
        "jest-each": "^24.9.0",
        "mqpacker": "^7.0.0",
        "postcss-cli": "^6.1.3",
        "postcss-custom-media": "^7.0.8",
        "postcss-custom-properties": "^9.0.2",
        "postcss-import": "^12.0.1",
        "postcss-pxtorem": "^4.0.1",
        "regenerator-runtime": "^0.13.3",
        "storybook-react-to-static-markup": "^0.1.1",
        "stylelint": "^12.0.1",
        "stylelint-config-standard": "^19.0.0",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.3"
    }
}
